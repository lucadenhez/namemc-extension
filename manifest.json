{
    "name" : "NameMC Account Creation Extension",
    "description" : "Chrome extension to add account creation date on NameMC!",
    "version" : "1.1",
    "manifest_version" : 2,
    "content_scripts" : [
        {
            "matches" : ["*://*.namemc.com/profile/*"],
            "js" : ["script.js"]
        }
    ],
    "icons" : {
        "16" : "obj-16x16.png",
        "32" : "obj-32x32.png",
        "48" : "obj-48x48.png",
        "128" : "obj-128x128.png"
    }
}
